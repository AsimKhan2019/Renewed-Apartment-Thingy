@model IEnumerable<RenewedApartmentThingy.ApartmentOwner>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Apartment Owner Name
        </th>
        <th>
            Apartment Owner Number
        </th>
        <th>
            Owner Start Date
        </th>
        <th>
            Owner End Date
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ApartmentOwnerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApartmentOwnerNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OwnerStartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OwnerEndDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ApartmentOwnerID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ApartmentOwnerID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ApartmentOwnerID })
            </td>
        </tr>
    }

</table>
